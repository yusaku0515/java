public class Main {
	public static void introduceOneself() {
	    String name = "岩田";
	    int age = 26;
	    double height = 170;
	    String zodiac = "戌";
	    System.out.println("名前は" + name + "と言います。");
	    System.out.println("年齢は" + age + "です。");
	    System.out.println("身長は" + height + "cmです。");
	    System.out.println("干支は" + zodiac + "です。");
	}
	public static void main(String[] args) {
		introduceOneself();
	}
}